<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>get-admin</servlet-name>
    <servlet-class>com.india.letsev.servlet.GetAdminByIdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>get-admin</servlet-name>
    <url-pattern>/getadmin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>get-all-admin</servlet-name>
    <servlet-class>com.india.letsev.servlet.GetAllAdminsServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>get-all-admin</servlet-name>
    <url-pattern>/getalladmins</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>edit-admin</servlet-name>
    <servlet-class>com.india.letsev.servlet.EditAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>edit-admin</servlet-name>
    <url-pattern>/updateadmin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>delete-admin</servlet-name>
    <servlet-class>com.india.letsev.servlet.DeleteAdminServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>delete-admin</servlet-name>
    <url-pattern>/deleteadmin</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>register-admin</servlet-name>
    <servlet-class>com.india.letsev.servlet.AdminRegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register-admin</servlet-name>
    <url-pattern>/registeradmin</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>get-user</servlet-name>
    <servlet-class>com.india.letsev.servlet.GetUserByIdServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>get-user</servlet-name>
    <url-pattern>/getuser</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>get-all-users</servlet-name>
    <servlet-class>com.india.letsev.servlet.GetAllUsersServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>get-all-users</servlet-name>
    <url-pattern>/getallusers</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>edit-user</servlet-name>
    <servlet-class>com.india.letsev.servlet.EditUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>edit-user</servlet-name>
    <url-pattern>/updateuser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>delete-user</servlet-name>
    <servlet-class>com.india.letsev.servlet.DeleteUserServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>delete-user</servlet-name>
    <url-pattern>/deleteuser</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>register-user</servlet-name>
    <servlet-class>com.india.letsev.servlet.UserRegistrationServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>register-user</servlet-name>
    <url-pattern>/registeruser</url-pattern>
  </servlet-mapping>




</web-app>
